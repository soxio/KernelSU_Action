CONFIG_ENV=config.env

KERNEL_SOURCE=https://github.com/xiaoleGun/android_kernel_xiaomi_wayne-4.19
KERNEL_SOURCE_BRANCH=twrp-12
KERNEL_CONFIG=vendor/wayne_defconfig
KERNEL_IMAGE_NAME=Image.gz-dtb
ARCH=arm64
ADD_LOCALVERSION_TO_FILENAME=false
EXTRA_CMDS:LD=ld.lld

# Clang
## Custom
USE_CUSTOM_CLANG=false
CUSTOM_CLANG_SOURCE=
CUSTOM_CLANG_BRANCH=

### if your set USE CUSTOM CLANG to false than DO NOT CHANGE CUSTOM CMDS
CUSTOM_CMDS:CLANG_TRIPLE=aarch64-linux-gnu-

## AOSP
CLANG_BRANCH=master-kernel-build-2022
CLANG_VERSION=r450784e

# GCC
ENABLE_GCC_ARM64=true
ENABLE_GCC_ARM32=true
USE_CUSTOM_GCC_64=false
CUSTOM_GCC_64_SOURCE=
CUSTOM_GCC_64_BRANCH=
CUSTOM_GCC_64_BIN=aarch64-linux-android-
USE_CUSTOM_GCC_32=false
CUSTOM_GCC_32_SOURCE=
CUSTOM_GCC_32_BRANCH=
CUSTOM_GCC_32_BIN=arm-linux-androideabi-

# KernelSU flags
ENABLE_KERNELSU=false
KERNELSU_TAG=v0.9.5
KSU_EXPECTED_SIZE=
KSU_EXPECTED_HASH=

# Configuration
DISABLE-LTO=false
DISABLE_CC_WERROR=false
ADD_KPROBES_CONFIG=false
ADD_OVERLAYFS_CONFIG=false
APPLY_KSU_PATCH=false
REMOVE_UNUSED_PACKAGES=true

# AnyKernel3
## AnyKernel3 Custom
USE_CUSTOM_ANYKERNEL3=false
CUSTOM_ANYKERNEL3_SOURCE=
CUSTOM_ANYKERNEL3_BRANCH=

# Ccache
ENABLE_CCACHE=true

# DTBO image
NEED_DTBO=false

# Build boot images
BUILD_BOOT_IMG=true
SOURCE_BOOT_IMAGE=https://my.microsoftpersonalcontent.com/personal/aee08a46058e5cc5/_layouts/15/download.aspx?UniqueId=257f9511-ec5d-4966-8a86-a6f71185c896&Translate=false&tempauth=v1e..Dbx_q61pGRnOcGIsufZdSBlkSs4-DqcYwLtXwvswzGEscRwhR9RFBSgf0lfjLH3ZKvVnu_5LCXa91-2VYULqV29JRihYOWIxmt3sCKUmaunjsHVClMbMUjr7GawmAYMGDexRoiGX80lLXbPGFaXhroJjbX_W49LP6CviLBCM88Aj0SdnG04purIWQyABc3zk5vokh_1tc8RYvLY6ivOhK6PXNgeSDb4NvIoPSaSE6iJ2o4YIICxZeFb7Or-ercYXiCAjvnr8DjYwIIPKV3nFp--U-kz2JjE23Zxlccy7NTfcNfvtuaEPhnZdv-TM7YveczwPSge0j9Re2awuPpE_TR3JxJ06C3urOsdfjxQEa0EQtJLljoo6M4CvKddsXsAIgww6HuEPWsqrISqKV7KxHCbGIvmDNgSR-MTEJR-rhVSjZBEp5DDOhBwmbcqKJi-B.OuHigE6hKb_5KubHJydqFU52u9JKxcqcd3g88asBsTg&ApiVersion=2.0
